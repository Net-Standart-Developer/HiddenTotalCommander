<Window x:Class="XAmlApp.RenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XAmlApp"
        mc:Ignorable="d"
        Title="RenameWindow" Height="165" Width="250" ResizeMode="NoResize"
        >
    <StackPanel>
        <TextBlock Margin="10 25 0 0" HorizontalAlignment="Left" Text="Введите имя элемента" FontWeight="Black" FontSize="14"/>
        <TextBox Margin="10 10 0 0" HorizontalAlignment="Left" Width="150" Height="30" Name="TextBoxForName"/>
        <Button Margin="10 10 0 0" Name="button" Content="Переименовать" Width="120" HorizontalAlignment="Left" Click="Button_Click"/>
    </StackPanel>
</Window>
